//SWITCH CASE

 let a=2;
switch(a){
    case 1:
        console.log("one")
        break;
    case 2:
        console.log("two")
        break;
    case 3:
        console.log("three")
        break;
    default:
        console.log("none")
    } 


//exercise
let player="rock";
let computer="scissors";

 switch(player){
   case "computer":
      console.log("tie");
      break;
   case "rock":
      if(computer=="scissors")
      console.log("player wins");
      else
      console.log("computer wins");
      break;
   default:
      console.log("no tie!")    
 }
